<robot name="JustinaLeftArm">
  <link name="shoulders_left_link">
    <visual>
      <origin xyz="0.035 0.085 0" rpy = "0 0 0"/>
      <geometry> <box size = "0.08 0.1 0.04"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.035 0.085 0" rpy = "0 0 0"/>
      <mass value = "0.1" />
      <inertia ixx = "0.1" iyy = "0.1" izz = "0.1" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
    
  <link name="left_arm_link1">
    <inertial>
      <origin xyz="0.0403 -0.015 0.0"/>
      <mass value = "0.35" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
    <visual>
      <origin xyz="0.0403 -0.015 0.0" rpy = "0 0 0"/> 
      <geometry>   
	<box size = "0.07 0.08 0.05"/> 
      </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
  </link>
  
  <link name="left_arm_link2">
    <visual>
      <origin xyz="0.105 0.0 0.0" rpy = "0 0 0"/> <!--position of prisma center of link2 wrt frame of link1.-->
      <geometry><box size = "0.21 0.04 0.04"/></geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.105 0.0 0.0" rpy = "0 0 0"/>
      <mass value = "0.2" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
  
  <link name="left_arm_link3">
    <visual>
      <origin xyz="0.21 0 0" rpy = "0 0 0"/>
      <geometry> <box size = "0.075 0.05 0.05"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.21 0 0" rpy = "0 0 0"/>
      <mass value = "0.18" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
  
  <link name="left_arm_link4">
    <visual>
      <origin xyz="0.09 0.0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.18 0.04 0.04"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.09 0.0 0.0" rpy = "0 0 0"/>
      <mass value = "0.18" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
  
  <link name="left_arm_link5">
    <visual>
      <origin xyz="0.18 0.0 0.0" rpy = "0 0 0"/> <!--position of prisma center of link2 wrt frame of link1.-->
      <geometry><box size = "0.045 0.05 0.05"/></geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.18 0.0 0.0" rpy = "0 0 0"/>
      <mass value = "0.18" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
  
  <link name="left_arm_link6">
    <visual>
      <origin xyz="0.05 0.0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.1 0.04 0.04"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.05 0.0 0.0" rpy = "0 0 0"/>
      <mass value = "0.15" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>
  
  <link name="left_arm_link7">
    <visual>
      <origin xyz="0.1 0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.06 0.04 0.04"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
    <inertial>
      <origin xyz="0.1 0 0.0" rpy = "0 0 0"/>
      <mass value = "0.15" />
      <inertia ixx = "0.3" iyy = "0.3" izz = "0.3" ixy = "0" ixz = "0" iyz = "0" />
    </inertial>
  </link>

  <link name="left_arm_grip_center">
    <visual>
      <origin xyz="0.0 0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.04 0.1 0.04"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
  </link>
  
  <link name="left_arm_grip_left">
    <visual>
      <origin xyz="0.065 0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.13 0.015 0.03"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
  </link>
  
  <link name="left_arm_grip_right">
    <visual>
      <origin xyz="0.065 0 0.0" rpy = "0 0 0"/>
      <geometry> <box size = "0.13 0.015 0.03"/> </geometry>
      <material name = "la_material"><color rgba="1.0 0.5 0 1"/></material>
    </visual>
  </link>
  
  <!--Remeber that origin is the transform from parent frame to child frame and joint is put on child frame -->
  <joint name="la_1_joint" type="revolute"> 
    <origin xyz="0.055 0.09 0" rpy="0 0 0"/>
    <parent link="shoulders_left_link"/>
    <child link="left_arm_link1"/>
    <axis xyz="0 -1 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <!--The next six origins are taken from the Denavit-Hartenberg parameters -->
  <joint name="la_2_joint" type="revolute">
    <origin xyz="0.0603 0 0" rpy="0 0 0"/><!--Transformation from link1 to link2 when theta1 = 0 -->
    <parent link="left_arm_link1"/>
    <child link="left_arm_link2"/>
    <axis xyz="0 0 -1" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_3_joint" type="revolute">
    <origin xyz="0.0 0 0" rpy="0 0 0"/><!--Transformation from link2 to link3 when theta2 = 0 -->
    <parent link="left_arm_link2"/>
    <child link="left_arm_link3"/>
    <axis xyz="1 0 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_4_joint" type="revolute">
    <origin xyz="0.27 0 0" rpy="0 0 0"/><!--Transformation from link3 to link4 when theta3 = 0 -->
    <parent link="left_arm_link3"/>
    <child link="left_arm_link4"/>
    <axis xyz="0 -1 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_5_joint" type="revolute">
    <origin xyz="0.0 0 0" rpy="0 0 0"/><!--Transformation from link4 to link5 when theta4 = 0 -->
    <parent link="left_arm_link4"/>
    <child link="left_arm_link5"/>
    <axis xyz="1 0 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_6_joint" type="revolute">
    <origin xyz="0.2126 0 0" rpy="0 0 0"/><!--Transformation from link5 to link6 when theta5 = 0 -->
    <parent link="left_arm_link5"/>
    <child link="left_arm_link6"/>
    <axis xyz="0 -1 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_7_joint" type="revolute">
    <origin xyz="0.0 0 0" rpy="0 0 0"/><!--Transformation from link6 to link7 when theta6 = 0 -->
    <parent link="left_arm_link6"/>
    <child link="left_arm_link7"/>
    <axis xyz="1 0 0" />
    <dynamics damping ="1.5" />
    <limit effort="1000" velocity="10" lower="-3.0" upper="3.0"/>
  </joint>
  <joint name="la_grip_center_joint" type="fixed">
    <origin xyz="0.13 0 0" rpy="0 0 0"/><!--Transformation from link7 to the center of the gripper -->
    <parent link="left_arm_link7"/> 
    <child link="left_arm_grip_center"/> 
  </joint>
  <joint name="la_grip_left" type="fixed">
    <origin xyz="0 -0.04 0" rpy="0 0 0"/><!--Transformation from link7 to the center of the gripper -->
    <parent link="left_arm_grip_center"/> 
    <child link="left_arm_grip_left"/> 
  </joint>
  <joint name="la_grip_right" type="fixed">
    <origin xyz="0 0.04 0" rpy="0 0 0"/><!--Transformation from link7 to the center of the gripper -->
    <parent link="left_arm_grip_center"/> 
    <child link="left_arm_grip_right"/> 
  </joint>
</robot>
